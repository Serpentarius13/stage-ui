{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "target": "esnext",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "strict": true,
    "declaration": true,
    "noEmit": true,
  },
  "exclude": [
    "node_modules",
    "scripts"
  ]
}