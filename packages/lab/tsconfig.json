{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "commonjs",
    "outDir": "../../build/lab",
    "allowJs": false,
    "declaration": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "alwaysStrict": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "strictBindCallApply": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": true,
    "types": ["@emotion/react"]
  },
  "exclude": [
    "node_modules"
  ]
}