{
  "name": "@stage-ui/core",
  "author": "Script Heads",
  "version": "0.3.4",
  "license": "MIT",
  "scripts": {
    "build": "node ../../scripts/make core && yarn build:types && yarn build:js",
    "build:js": "babel ./ --out-dir ../../build/core --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "gen:declarations": "tsc --project tsconfig.docs.json",
    "lint": "eslint --config .eslintrc.json --fix '**/*.+(js|jsx|ts|tsx)'"
  },
  "dependencies": {
    "@emotion/react": ">=11",
    "imask": "^6.0.1",
    "moment": "^2.29.1",
    "rifm": "^0.12.0"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}