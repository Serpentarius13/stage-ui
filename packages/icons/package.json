{
  "name": "@stage-ui/icons",
  "author": "Project one",
  "version": "0.3.1-beta4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/script-heads/StageUI.git",
    "directory": "packages/icons"
  },
  "homepage": "https://stageui.project1.io",
  "bugs": "https://github.com/script-heads/StageUI/issues",
  "scripts": {
    "build": "node ../../scripts/make icons && yarn build:types && yarn build:js",
    "build:js": "babel ./ --out-dir ../../build/icons --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
    "build:types": "tsc --emitDeclarationOnly",
    "gen": "node ./scripts/generate",
    "lint": "eslint --config .eslintrc.json --fix '**/*.+(js|jsx|ts|tsx)'"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@stage-ui/system": "^0.2.17"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}