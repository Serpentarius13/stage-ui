{
  "name": "@stage-ui/icons",
  "description": "Flexible UI Framework declared by your own Design System.",
  "version": "0.3.37",
  "license": "MIT",
  "homepage": "https://stageui.shds.io/",
  "bugs": "https://github.com/script-heads/stage-ui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/script-heads/stage-ui.git",
    "directory": "packages/icons"
  },
  "scripts": {
    "build": "node ../../scripts/make icons && yarn build:types && yarn build:js",
    "build:js": "babel ./ --out-dir ../../build/icons --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
    "build:types": "tsc --emitDeclarationOnly",
    "gen": "node ./scripts/generate",
    "gen:declarations": "tsc --project tsconfig.docs.json",
    "lint": "eslint --config .eslintrc.json --fix '**/*.+(js|jsx|ts|tsx)'"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}